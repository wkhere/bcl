var service1_default_port    = 8400
var service1_local_base_port = 9400
var domain = "foo.org"
var z0 = -z1 + +0      # forward decl
var z1 = service1_local_base_port - service1_default_port
var z2 = -1 - (---10/2-1) + z1
var s  = "sth" + 1 + "-" + domain
var cond  = true
var cond2 = false
var cond_big = not not not not cond


tunnel "hostyhost-service1" {
	local_port  = service1_local_base_port
	remote_port = service1_default_port
	host = "hosty." + domain
	enabled = not cond2
}

service1 "another-service1" {
	host = "yet" + "." + "another.com"
	local_port  = service1_local_base_port + 1
	remote_port = service1_default_port
	enabled  = cond
	prepared = true
	started  = cond_big
	str = "a" * 4
	u = 4+3*2
	v = z0
	x = z2 - z1
	y = 3 - ---8/4 - (+1-3/2)
}
